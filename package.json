{
  "name": "eatwell",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "lint": "eslint .",
    "test": "jest",
    "clean": "rm -rf node_modules package-lock.json && npx expo install",
    "deps": "npx expo install",
    "deps:check": "npx expo install --check",
    "deps:fix": "npx expo install --fix",
    "doctor": "npx expo-doctor",
    "update": "npx expo update"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^2.0.0",
    "@supabase/supabase-js": "^2.0.0",
    "expo": "~49.0.15",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-router": "~2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^5.0.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "typescript": "^5.1.3"
  },
  "private": true,
  "resolutions": {
    "react": "18.2.0",
    "react-native": "0.72.6"
  },
  "// Helpful commands": {
    "install new package": "npx expo install [package-name]",
    "check deps": "npx expo-doctor",
    "fix deps": "npm run deps:fix",
    "clean reinstall": "npm run clean"
  }
}
